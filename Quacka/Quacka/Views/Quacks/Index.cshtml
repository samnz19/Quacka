@model IEnumerable<Quacka.Models.Quack>

@{
    ViewBag.Title = "Index";
}

<h2>Quacks</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Body)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CreatedAt)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            <strong>@Html.DisplayFor(modelItem => item.ApplicationUser.UserName)</strong> @Html.DisplayFor(modelItem => item.Body)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CreatedAt)
        </td>

    </tr>
}

</table>
